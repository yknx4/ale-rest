{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "User",
    "type": "object",
    "tableName": "users",
    "additionalProperties": false,
    "primaryKey": "id",
    "properties": {
        "first_name": {
            "fake": "name.firstName",
            "type": "string"
        },
        "last_name": {
            "fake": "name.lastName",
            "type": "string"
        },
        "username": {
            "fake": "internet.userName",
            "type": "string"
        },
        "email": {
            "fake": "internet.email",
            "type": "string",
            "format": "email"
        },
        "created_at": {
          "anyOf": [
            {
              "type": "null"
          },
          {
              "type": "object"
          },
          {
              "type": "string",
              "format": "date-time"
          }
      ]
      },
      "updated_at": {
          "anyOf": [
            {
              "type": "null"
          },
          {
              "type": "object"
          },
          {
              "type": "string",
              "format": "date-time"
          }
      ]
      }
  },
  "required": ["first_name", "last_name"]
}
