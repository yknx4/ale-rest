{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://www.ale.world/schemas/user",
    "title": "User",
    "type": "object",
    "tableName": "users",
    "additionalProperties": false,
    "description": "awesome description",
    "primaryKey": "id",
    "properties": {
        "id": {
            "type": "integer"
        },
        "first_name": {
            "description": "whata",
            "x-faker": "name.firstName",
            "type": "string"
        },
        "last_name": {
            "x-faker": "name.lastName",
            "type": "string"
        },
        "username": {
            "x-faker": "internet.userName",
            "type": "string"
        },
        "email": {
            "x-faker": "internet.email",
            "type": "string",
            "format": "email"
        },
        "created_at": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "object"
                },
                {
                    "type": "string",
                    "format": "date-time"
                }
            ]
        },
        "updated_at": {
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "object"
                },
                {
                    "type": "string",
                    "format": "date-time"
                }
            ]
        }
    },
    "required": [
        "email",
        "first_name",
        "last_name",
        "username"
    ]
}